{
    "buildCommand": "cd backend && pip install -r requirements.txt",
    "outputDirectory": "frontend",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/app/main.py"
        }
    ],
    "builds": [
        {
            "src": "backend/app/main.py",
            "use": "@vercel/python"
        }
    ]
}