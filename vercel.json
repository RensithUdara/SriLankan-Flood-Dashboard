{
    "builds": [
        {
            "src": "backend/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/app/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}